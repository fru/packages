{
  "version": "1.5.0",
  "name": "@fru/astro-sprite",
  "description": "Sprite-based SVGs in Astro",
  "author": "Florian Rueberg",
  "license": "MIT",
  "homepage": "rueberg.eu/astro-sprite",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fru/packages.git"
  },
  "bugs": {
    "url": "https://github.com/fru/packages/issues"
  },
  "keywords": [
    "SVG",
    "Astro",
    "Sprite",
    "Spritesheet",
    "Icon",
    "Icons",
    "Iconify",
    "Svg Stack"
  ],
  "files": [
    "dist/**",
    "src/astro-entry.ts",
    "src/icon.astro"
  ],
  "type": "module",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    },
    "./src/astro-entry.ts": "./src/astro-entry.ts",
    "./src/icon.astro": "./src/icon.astro"
  },
  "scripts": {
    "build": "unbuild",
    "test": "vitest",
    "coverage": "vitest run --coverage --coverage.reporter=text",
    "prepublishOnly": "npm run build",
    "ci-test": "vitest --run",
    "ci-coverage": "vitest run --coverage --coverage.reporter=lcov --coverage.reporter=json"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "@vitest/coverage-v8": "^4.0.6",
    "typescript": "~5.9.3",
    "unbuild": "^3.6.1",
    "vitest": "^4.0.5"
  },
  "dependencies": {
    "@iconify/types": "^2.0.0",
    "@iconify/utils": "^3.0.2",
    "svgson": "^5.3.1"
  },
  "peerDependencies": {
    "astro": "^5.15.3",
    "astro-integration-kit": "^0.19.1"
  }
}
